rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    match /bookings/{document} {
      allow read, write: if true;
    }
    match /mobile-mountain-bookings/{document} {
      allow read, write: if true;
    }
    match /website-content/{document} {
      allow read, write: if true;
    }
    match /{document=**} {
      allow read, write: if false;
    }
  }
}
